{
  "name": "remotion-studio",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write .",
    "create:project": "tsx scripts/create-project.ts",
    "dev": "echo 'Use: pnpm -C apps/<name> run dev'",
